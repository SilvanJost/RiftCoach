{
    "manifest_version": 1,
    "type": "WebApp",
    "meta": {
        "name": "Rift Coach",
        "version": "1.0.0",
        "minimum-overwolf-version": "0.77.10",
        "author": "Silvan Jost",
        "description": "",
        "dock_button_title": "Rift Coach",
        "icon": "icons/IconMouseOver.png",
        "icon_gray": "icons/IconMouseNormal.png",
        "launcher_icon": "icons/DesktopIcon.ico",
        "window_icon": "icons/TaskbarIcon.png"
    },
    "data": {
        "start_window": "desktop",
        "windows": {
            "desktop": {
                "file": "desktop.html",
                "desktop_only": true,
                "native_window": true,
                "resizable": true,
                "transparent": true,
                "override_on_update": true,
                "size": {
                    "width": 1212,
                    "height": 699
                },
                "min_size": {
                    "width": 1212,
                    "height": 699
                }
            }
        }
    }
}